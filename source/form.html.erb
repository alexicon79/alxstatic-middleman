<div class="welcome">
  <h1>Mys fomsich</h1>

	<% form_tag 'http://www.test.com/test.php', :class => 'alx-form', :method => 'post' do %>
	  <% field_set_tag do %>
	    <p>
	      <%= label_tag :name, :class => 'first' %>
	      <%= text_field_tag :name, :placeholder => 'hej' %>
	    </p>
	    <p>
	      <%= label_tag :email, :class => 'first' %>
	      <%= email_field_tag :email, :placeholder => 'email' %>
	    </p>
	    <p>
	      <%= check_box_tag :confirm_delete %>
	    </p>
	  <% end %>
	  <% field_set_tag(:class => 'buttons') do %>
	    <%= submit_tag "Remove" %>
	  <% end %>
	<% end %>

